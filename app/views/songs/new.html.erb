<h1>Song Form</h1>

<%= form_for @song do |f| %>
    <label for="song_name">My song name:</label>
    <%= f.text_field :name %>
    <label for="song_artist">My song artist id:</label>
    <%= f.text_field :artist_id %>
    <label for="song_genre">My song genre id:</label>
    <%= f.text_field :genre_id %>
    <%= f.submit %>
<% end %>